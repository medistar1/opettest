<?xml version='1.0' encoding='utf-8'?>
<widget id="com.whiteball.software" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ball2</name>
    <description>
        A ball software.
    </description>
    <author email="winroulettespinsoftware@gmail.com" href="http://winroulettespin.info">
        WinRoulette Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-targetSdkVersion" value="24" />
    <preference name="orientation" value="landscape" />
    <preference name="prerendered-icon" value="true"/>
   <icon src="img/icon.png" gap:role="default" />
    <splash src="splash.png" gap:role="default" />
    <platform name="android">
        <icon qualifier="ldpi" src="res/mitmap-ldpi-icon.png" />
        <icon qualifier="mdpi" src="res/mitmap-mdpi-icon.png" />
        <icon qualifier="hdpi" src="res/mitmap-hdpi-icon.png" />
        <icon qualifier="xhdpi" src="res/mitmap-xhdpi-icon.png" />
        <icon qualifier="xxhdpi" src="res/mitmap-xxhdpi-icon.png" />
        <icon qualifier="xxxhdpi" src="res/mitmap-xxxhdpi-icon.png" />
        <splash qualifier="land-ldpi" src="res/screen-ldpi-landscape.png" />
        <splash qualifier="land-mdpi" src="res/screen-mdpi-landscape.png" />
        <splash qualifier="land-hdpi" src="res/screen-hdpi-landscape.png" />
        <splash qualifier="land-xhdpi" src="res/screen-xhdpi-landscape.png" />
        <splash qualifier="port-ldpi" src="res/screen-ldpi-portrait.png" />
        <splash qualifier="port-mdpi" src="res/screen-mdpi-portrait.png" />
        <splash qualifier="port-hdpi" src="res/screen-hdpi-portrait.png" />
        <splash qualifier="port-xhdpi" src="res/screen-xhdpi-portrait.png" />
       
        
    </platform>
   
</widget>
